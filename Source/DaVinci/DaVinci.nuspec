<?xml version="1.0"?>
<package xmlns="http://schemas.microsoft.com/packaging/2011/08/nuspec.xsd">
  <metadata>
    <id>DaVinci</id>
    <version>$version$</version>
    <title>DaVinci</title>
    <authors>Johannes Schmitt</authors>
    <owners>Johannes Schmitt</owners>
    <projectUrl>https://github.com/johannesschmitt/DaVinci</projectUrl>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>DaVinci code analyzer</description>
    <copyright>2015 Johannes Schmitt</copyright>
    <tags>DaVinci, analyzers</tags>
    <frameworkAssemblies>
      <frameworkAssembly assemblyName="System" targetFramework="" />
    </frameworkAssemblies>
  </metadata>
  <files>
    <file src="$targetPath$" target="tools\analyzers\" />
    <file src="tools\*.ps1" target="tools\" />
  </files>
</package>